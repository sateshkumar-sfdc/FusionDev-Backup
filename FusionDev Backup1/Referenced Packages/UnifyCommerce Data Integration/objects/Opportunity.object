    <validationRules>
        <fullName>Required_Account_for_Opportunities</fullName>
        <active>true</active>
        <description>This validation rule ensures that the Account is always required when an Opportunity is created. As a result, Opportunities which are not related to an Account cannot be inserted via integrations.</description>
        <errorConditionFormula>AccountId = null &amp;&amp;  DW_ID__c != null</errorConditionFormula>
        <errorMessage>The Opportunity MUST be associated with an Account in order to be created. Perform the necessary changes and please try again.</errorMessage>
