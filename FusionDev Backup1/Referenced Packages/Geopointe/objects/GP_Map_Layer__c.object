        <fullName>Data_Layer_Detail_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	LOWER(TEXT(Type__c)) = &apos;datalayer&apos;,
	ISBLANK(Detail__c)
)</errorConditionFormula>
        <errorDisplayField>Detail__c</errorDisplayField>
        <errorMessage>Detail__c field is required for data layers.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Data_Layer_Folder_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	LOWER(TEXT(Type__c)) = &apos;datalayer&apos;,
	AND(NOT(Personal__c), ISBLANK(Folder__c))
)</errorConditionFormula>
        <errorDisplayField>Folder__c</errorDisplayField>
        <errorMessage>Data layers must be associated with a folder if they are not personal.</errorMessage>
    </validationRules>
