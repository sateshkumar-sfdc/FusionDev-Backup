<apex:page controller="geopointe.Viz_Controller" showHeader="true" sidebar="{!userSettings.geopointe__Hide_Sidebar__c = false}" tabStyle="Visualize__tab" title="Geopointe - Visualize {!IF(userAccessPlusSKSetup,' & Analytics','')}">
	
