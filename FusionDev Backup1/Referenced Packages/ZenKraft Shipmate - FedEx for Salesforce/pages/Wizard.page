                                                                <apex:selectOptions value="{!Section3.AddressSourceOptions}"/>
                                                            </apex:selectList>
                                                            <apex:image id="loadingAddressSourceSelect" styleClass="loadingAddressSourceSelect" value="{!$Resource.zkfedex__LoadingImage2}"/>

