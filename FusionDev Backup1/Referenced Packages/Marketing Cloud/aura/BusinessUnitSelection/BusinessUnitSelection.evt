<aura:event type="COMPONENT" description="This event is fired when a user must select a business unit to integrate with.">
    <aura:attribute name="businessUnits" type="JSON" description="JSON object with a list of BUs and their information."/>
    <aura:attribute name="username" type="String" description="The username the user just entered. This should not be written out or stored in a public variable"/>
    <aura:attribute name="password" type="String" description="The password the user just entered. This should not be written out or stored in a public variable"/>
</aura:event>