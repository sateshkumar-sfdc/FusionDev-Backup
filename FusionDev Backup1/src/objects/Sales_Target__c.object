    <validationRules>
        <fullName>Product_Category_Coolers_or_All_Other</fullName>
        <active>false</active>
        <description>Do not allow save of record if product category is not equal to &quot;Coolers&quot; or &quot;All Other&quot;</description>
        <errorConditionFormula>AND(
 Product_Category__c &lt;&gt; &quot;All Other&quot;,
 Product_Category__c &lt;&gt; &quot;Coolers&quot;
)</errorConditionFormula>
        <errorDisplayField>Product_Category__c</errorDisplayField>
        <errorMessage>Product category should equal &quot;Coolers&quot; or &quot;All Other&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Start_Date_End_Date_Cannot_be_same_date</fullName>
        <active>true</active>
