    <validationRules>
        <fullName>Need_By_Date_Cannot_Be_Before_Available</fullName>
        <active>false</active>
        <errorConditionFormula>if(Not(IsBlank(PricebookEntry.Product2.Availability_Date__c))
&amp;&amp; Need_By_Date__c &lt; PricebookEntry.Product2.Availability_Date__c,true,false)</errorConditionFormula>
        <errorMessage>Need by date cannot be before the availability date for a product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_NeedByDate_Before_Oppty_NeedByDate</fullName>
        <active>false</active>
        <description>Opportunity product need by date cannot be before opportunity need by date</description>
        <errorConditionFormula>Need_By_Date__c &lt;  Opportunity.Need_By_Date__c</errorConditionFormula>
        <errorDisplayField>Need_By_Date__c</errorDisplayField>
        <errorMessage>Product Need by Date must be after Opportunity Need by Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_New_Account_Limited_Product</fullName>
        <active>false</active>
        <description>If account create date is less than 60 days from today, only allow product category of &quot;Coolers&quot; to be selected.</description>
        <errorConditionFormula>AND(
  Opportunity.Account.Limited_Product_Override__c = FALSE,
  Today()- Opportunity.Account.Established_Date__c  &lt; 60,
  Product2.Product_Category__c &lt;&gt; &quot;Coolers&quot;
)</errorConditionFormula>
        <errorMessage>This account was created within the last 60 days and is only authorized to order Coolers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>OLI_Qty_Must_be_Mult_of_Min_Order_Qty</fullName>
        <active>true</active>
        <description>The entered quantity must be a multiple of the minimum order quantity for the selected product.</description>
        <errorConditionFormula>IF(MOD(Quantity,PricebookEntry.Product2.Minimum_Order_Quantity__c)&gt;0, true, false)</errorConditionFormula>
        <errorMessage>The entered quantity must be a multiple of the minimum order quantity for the selected product.</errorMessage>
    </validationRules>
