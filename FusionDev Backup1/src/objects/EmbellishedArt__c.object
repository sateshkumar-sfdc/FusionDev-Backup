    <validationRules>
        <fullName>ArtNameFieldValidation</fullName>
        <active>true</active>
        <description>[EmbArts-03] : This rule validates the Art Name field to have only alphanumeric,hyphen&apos;s or space contained in it.</description>
        <errorConditionFormula>IF( AND( 
NOT(ISBLANK(ArtName__c)) , 
NOT( REGEX( ArtName__c , &quot;^[#()&amp;/a-zA-Z0-9&apos;.-](?!.*--)(?!.*&amp;&amp;)(?!.*  )(?!.*#)(?!.*&apos;&apos;)(?!.*//)[ /()&amp;a-zA-Z0-9&apos;.-]*[a-zA-Z0-9)]$&quot;)) 
),true,false)</errorConditionFormula>
        <errorDisplayField>ArtName__c</errorDisplayField>
        <errorMessage>[EmbArts-03] :You can enter only alphanumeric characters or some special characters. 
Note : Cannot contain two spaces or similar special characters next to each other.
Correct Format : [Text/Numeric/Characters] [Text/Numeric/Characters]</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RestrictLicensedPropertyField</fullName>
        <active>true</active>
        <description>[EmbArts-02]-This rule restricts Licensed Property field based on product type (Is Premium) .</description>
        <errorConditionFormula>AND( 
NOT(ISBLANK(LicensedProperty__c)), 
NOT(ISPICKVAL(IsPremium__c, &apos;Yes&apos;)) 
)</errorConditionFormula>
        <errorDisplayField>LicensedProperty__c</errorDisplayField>
        <errorMessage>[EmbArts-02] : Licensed Property is available only for premium designs.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>YETIKeyFieldValidation</fullName>
        <active>true</active>
        <description>[EmbArts-01] : This rule validates the YETI key field to have only alphanumeric or hyphen&apos;s contained in it.</description>
        <errorConditionFormula>IF(CONTAINS(ArtYETIKey__c , &apos;-&apos;), AND( 
NOT(ISBLANK(ArtYETIKey__c )) , 
NOT( REGEX( ArtYETIKey__c , &quot;^[a-zA-Z0-9](?!.*--)[-a-zA-Z0-9]*[a-zA-Z0-9]$&quot;)) 
),AND( 
NOT(ISBLANK(ArtYETIKey__c )) , 
NOT( REGEX( ArtYETIKey__c , &quot;[A-Za-z0-9]+&quot;)) 
))</errorConditionFormula>
        <errorDisplayField>ArtYETIKey__c</errorDisplayField>
        <errorMessage>[EmbArts-01]-You can enter only alphanumeric characters or &apos;-&apos;. 
Note : Cannot contain two hyphens next to each other or a space between words. 
Correct Format : [Text/Numeric]-[Text/Numeric]</errorMessage>
    </validationRules>
